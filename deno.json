{
  "tasks": {
    "compile-win": "deno compile --target x86_64-pc-windows-msvc --v8-flags=--max-old-space-size=8000 --output repro-win.exe main.ts",
    "compile-mac": "deno compile --target x86_64-apple-darwin --output --v8-flags=--max-old-space-size=8000 no-issue-mac main.ts",
    "compile-linux": "deno compile --target x86_64-unknown-linux-gnu --v8-flags=--max-old-space-size=8000 --output no-issue-linux main.ts"
  }
}
